unit Unit1;

interface

uses
  Unit2, Unit3, Unit4, Unit5,
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls;

type
  TFormMenu = class(TForm)
    BtnVInterna: TButton;
    BtnVExterna: TButton;
    BtnVMOIP: TButton;
    Button1: TButton;
    procedure BtnVInternaClick(Sender: TObject);
    procedure BtnVExternaClick(Sender: TObject);
    procedure BtnVMOIPClick(Sender: TObject);
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FormMenu: TFormMenu;

implementation

{$R *.dfm}

procedure TFormMenu.BtnVExternaClick(Sender: TObject);
begin
  FormVendaExterna.Show;
end;

procedure TFormMenu.BtnVInternaClick(Sender: TObject);
begin
  FormVendaInterna.Show;
end;

procedure TFormMenu.BtnVMOIPClick(Sender: TObject);
begin
  FormVendaMOIP.Show;
end;

procedure TFormMenu.Button1Click(Sender: TObject);
begin
  FormFormPedidoVenda.Show;
end;

end.
